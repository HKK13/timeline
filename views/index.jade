    div#loadingBackground
        div#addItemDialog
            form#addItemForm(enctype="multipart/form-data")
                h3 New Item
                input.datepickers(type="text", placeholder='Begin Date dd/mm/yyyy' name="bDate" required)
                input.datepickers(type='text', placeholder='End Date dd/mm/yyyy' name="eDate" required)
                input(type='text', placeholder='Title' name="title" required)
                textarea(placeholder='Description' name="description" required)
                input(type='text', placeholder='JIRA ID' name="jiraId" required)
                select(name="timelineName" required)
                    option(value='Boogle') Boogle
                    option(value='ClipUniverse') ClipUniverse
                    option(value='eTeam') eTeam
                    option(value='EOS') EOS
                    option(value='InBetween') InBetween
                    option(value='MAM') MAM
                    option(value='Mantra') Mantra
                    option(value='PDP') PDP
                    option(value='PICenter') PICenter
                    option(value='TMS') TMS
                textarea.tags( placeholder="Type Team Members" name="teamMembers")
                label.fileInput-label Browse
                    input(type="file" name="photo" accept=".jpg,.png")
                input(type='submit', value='Submit')
        div#editItemDialog
            form#editItemForm(action="/EditItem" method="POST" enctype="multipart/form-data")
                h3 Edit Item
                input.datepickers(type="text", placeholder='Begin Date dd/mm/yyyy' name="bDate" )
                input.datepickers(type='text', placeholder='End Date dd/mm/yyyy' name="eDate" )
                input(type='text', placeholder='Title' name="title" )
                textarea(placeholder='Description' name="description" )
                input(type='text', placeholder='JIRA ID' name="jiraId" )
                input(style='display: none;' type='text' name="postId")
                input(style='display: none;' type='text' name="postTimeline")
                textarea.tags( placeholder="Type Team Members" name="teamMembers")
                label.fileInput-label Change Picture
                    input#editPicture(type="file" name="photoEdit" accept=".jpg,.png")
                input(type='submit' value='Submit')
                input#deletePostButton(type='button', value='DELETE')

    div#dialog-confirm(title="Remove item?")
        p   Well well well removing an item... Are you sure?


